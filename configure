ac_unique_file="ChangeLog"
ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS THREAD_BIN_DIR JIKES JAVAC JAVA JAVA_G JAVAH JAR JDB DDD GDB CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT SHLIB_CFLAGS SHLIB_LD SHLIB_LD_LIBS SHLIB_PREFIX SHLIB_SUFFIX SHLIB_VERSION TCL_BUILD_LIB_SPEC TCL_CFLAGS TCL_DEFS TCL_LIBS TCL_VERSION TCL_SRC_DIR TCL_BIN_DIR TCL_LD_SEARCH_FLAGS TCL_DBGX TCLJAVA_DBGX JAVA_INCLUDE_FLAGS JAVA_LIB_FLAGS JAVA_LIB_RUNTIME_PATH TCLBLEND_LD_PRELOAD TCLBLEND_LD_BIND_NOW WISH TCLSH TCL_BIN_TCLSH TCL_INSTALL_LIB_DIR BUILD_DIR TOP_DIR TOP_DIR_NATIVE BUILD_DIR_NATIVE THREAD_BIN_DIR_NATIVE WIN32_HOST JAVA_CLASSPATH JAVA_FLAGS JAVAC_FLAGS JAVAC_D_FLAG JAVA_G_FLAGS JDB_ATTACH_FLAGS JAR_EXTRACT_FLAGS JAR_COMPRESS_FLAGS JAR_NOCOMPRESS_FLAGS TCLJAVA_VERSION TCLJAVA_NODOT_VERSION JAVA_NATIVE PREFIX_NATIVE EXEC_PREFIX_NATIVE TCLSH_NATIVE JAVA_CLASSPATH_NATIVE JAVA_LIB_RUNTIME_PATH_NATIVE TCLJAVA LIBOBJS LTLIBOBJS'
ac_subst_files=''

