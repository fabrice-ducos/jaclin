tcltest::test testtjc-package-1.3 {TJC::package} {
    package require TJC
    # Loading test.tjcext package will source
    # f1.tcl, f2.tcl, and f3.tcl from
    # the tjcext.jar file.
    foreach cmd [info commands fp*] {
        rename $cmd {}
    }
    TJC::package test.tjcext
    set results [lsort [info commands fp*]]
    foreach cmd $results {
        rename $cmd {}
    }
    set results
} {fp1 fp2 fp3}

